  "version": "0.2.0",
  "configurations": [
    {
      "name": "Get Oauth",
      "type": "debugpy",
      "request": "launch",
      "program": "ytplaylists.py",
      "console": "integratedTerminal",
      //"justMyCode": false,
      "args": ["oauth"]
    },
    {
      "name": "List problems",
      "type": "debugpy",
      "request": "launch",
      "program": "ytplaylists.py",
      "console": "integratedTerminal",
      //"justMyCode": false,
      "args": ["problems", "Volleyball Dirty", "6"]
    },
    {
      "name": "Sort playlist",
      "type": "debugpy",
      "request": "launch",
      "program": "ytplaylists.py",
      "console": "integratedTerminal",
      //"justMyCode": false,
      "args": ["sort", "Volleyball Dirty", "Volleyball Temp"]
    },
    {
      "name": "Clean explicit playlist",
      "type": "debugpy",
      "request": "launch",
      "program": "ytplaylists.py",
      "console": "integratedTerminal",
      //"justMyCode": false,
      "args": [
        "clean",
        "Volleyball Dirty",
        "Volleyball Clean",
        "Volleyball Temp"
      ]
    },
    {
      "name": "Compare playlists",
      "type": "debugpy",
      "request": "launch",
      "program": "ytplaylists.py",
      "console": "integratedTerminal",
      //"justMyCode": false,
      "args": ["compare", "Volleyball Dirty", "Volleyball Temp"]
    }
  ]
}
